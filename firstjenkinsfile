pipeline {
   agent any
      
      stages {
        stage("Print") {
          steps {
          echo "Hello world"
                 }
              }
        stage("checkout") {
          steps {
             checkout([$class: 'GitSCM', branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/jagadeeshvarma169/repositories01.git']]])
                 }
              }
         stage("Build") {
          steps {
             sh 'mvn clean install'
                 }
              }
           }
          post {
           always {
            echo "Thank you for running the job"
         }
      }
    }
